<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-3.0.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <context:annotation-config />
    <aop:aspectj-autoproxy />

    <bean id="Pages" class="com.assignmentPersado.utilities.Pages" />
    <bean id="AmazonHomePage" class="com.assignmentPersado.pageObjects.AmazonHomePage" />
    <bean id="BasePage" class="com.assignmentPersado.pageObjects.BasePage" />
    <bean id="CartPage" class="com.assignmentPersado.pageObjects.CartPage" />
    <bean id="GiftCardsPage" class="com.assignmentPersado.pageObjects.GiftCardsPage" />
    <bean id="GiftCarsProductsPage" class="com.assignmentPersado.pageObjects.GiftCardsProductPage" />

</beans>